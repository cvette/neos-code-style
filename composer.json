{
    "name": "cvette/neos-code-style",
    "description": "Linter for Neos Fusion",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Vette",
            "email": "github@christianvette.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vette\\Neos\\CodeStyle\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "require": {
        "symfony/console": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/config": "^5.2",
        "php": "^7.3||^8.0"
    },
    "require-dev": {
        "vimeo/psalm": "^4.6",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "compile": "box compile",
        "test": "phpunit tests"
    },
    "bin": [
        "bin/neoscs"
    ]
}
