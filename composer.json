{
    "name": "cvette/neos-code-style",
    "description": "Linter for Neos Fusion",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Vette",
            "email": "github@christianvette.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vette\\Neos\\CodeStyle\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1",
            "ext-dom": "8.1",
            "ext-simplexml": "8.1",
            "ext-xml": "8.1",
            "ext-xmlwriter": "8.1"
        }
    },
    "require": {
        "symfony/console": "^6.4",
        "symfony/filesystem": "^6.4",
        "symfony/yaml": "^6.4",
        "symfony/config": "^6.4",
        "php": "^8.1"
    },
    "require-dev": {
        "vimeo/psalm": "^5.26",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "compile": "box compile",
        "test": "phpunit tests"
    },
    "bin": [
        "bin/neoscs"
    ]
}
